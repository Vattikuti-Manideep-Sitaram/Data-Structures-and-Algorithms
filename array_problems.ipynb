   "source": [
    "### Find Second Smallest and Second Largest Element in an array "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "id": "e3b5a47e",
   "metadata": {},
   "outputs": [],
   "source": [
    "def find_the_elements(nums:list):\n",
    "    if len(nums) == 1:\n",
    "        return -1\n",
    "    \n",
    "    nums.sort()\n",
    "    return (nums[1],nums[-2])"
   ]
  },
